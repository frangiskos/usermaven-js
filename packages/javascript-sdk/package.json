{
  "name": "@usermaven/sdk-js",
  "version": "0.1.0",
  "description": "Usermaven JavaScript SDK",
  "main": "dist/usermaven-js-sdk.umd.js",
  "module": "dist/usermaven-js-sdk.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "example:build": "tsc examples/test.ts --outDir examples",
    "example:serve": "vite --config vite.config.ts",
    "start": "npm run example:build && concurrently \"npm run dev\" \"npm run example:serve\""
  },
  "keywords": [
    "usermaven",
    "analytics",
    "tracking"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }
}
