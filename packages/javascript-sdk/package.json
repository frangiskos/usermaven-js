{
  "name": "@usermaven/sdk-js",
  "version": "0.1.0",
  "description": "Usermaven JavaScript SDK",
  "main": "dist/usermaven-js-sdk.umd.js",
  "module": "dist/usermaven-js-sdk.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "mock": "node server/mockServer.js",
    "example:build": "tsc examples/test.ts --outDir examples",
    "example:serve": "vite --config vite.config.ts",
    "start": "concurrently \"npm run example:serve\" \"npm run mock\" \"npm run dev\""
  },
  "keywords": [
    "usermaven",
    "analytics",
    "tracking"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "body-parser": "^1.20.3",
    "chalk": "^5.3.0",
    "colors": "^1.4.0",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "eslint": "^8.37.0",
    "express": "^4.21.0",
    "morgan": "^1.10.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3",
    "vite": "^5.4.8",
    "winston": "^3.14.2"
  }
}
